### [meta]
dir = "/var/lib/influxdb/meta"

### [data]
dir = "/var/lib/influxdb/data"
wal-dir = "/var/lib/influxdb/wal"

### [http]
enabled = true
bind-address = "127.0.0.1:8086"
auth-enabled = {{ influxdb_auth_enabled | default(false) }}

### [admin]
enabled = false

### [monitor]
store-enabled = false
